<h1>Editing Course</h1>

<div class="col-md-4">
  <%= render 'form' %>
  </br>
  <div class="btn-group">
    <%= link_to 'Show', @course, class: "btn btn-info-outline" %>
    <%= link_to 'Back', courses_path, class: "btn btn-warning-outline" %>
    <%= link_to 'Destroy', @course, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger-outline" %>
  </div>
</div>
