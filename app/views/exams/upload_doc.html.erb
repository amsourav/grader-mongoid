<p>
  Upload Doc with <%= @exam.page_size %> pages.
</p>
<%= simple_form_for(@doc, :url => upload_doc_submit_course_exam_path(@course, @exam)) do |f| %>
    <%= f.error_notification %>
    <div class="form-inputs">
      <div class="col-md-4">
        <%= f.file_field :file %>
      </div>
    </div>

    <div class="form-actions">
        <%= f.submit %>
    </div>
<% end %>
